<h1>Requests#show</h1>
<p><%=@job.user.first_name%></p>
<p><%=@job.category%></p>
<p><%= @request.start_at.strftime('%A, %e %B, from %l %P') %> to <%= @request.end_at.strftime('%l %P') %></p>
<%= link_to "Send #{@request.destinator.first_name} a message",job_request_messages_path(@job, @request), class:'btn-jobme' %>

